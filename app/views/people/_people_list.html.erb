<table>
	<thead>
		<tr>
			<th>Nome</th>
      <th>Nomes do meio</th>
			<th>Ações</th>
		</tr>
	</thead>
	<tbody>
		<% @people.each do |person| %>
			<tr>
        <td><%= link_to "#{person.first_name} #{person.last_name}", edit_person_path(person) %></td>
        <td><%= link_to person.middle_names, edit_person_path(person) %></td>
				<td><%= link_to 'eliminar', person, method: :delete %></td>
			</tr>
		<% end %>
	</tbody>
</table>
